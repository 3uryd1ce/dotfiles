URxvt*loginShell:	true
URxvt*scrollBar:	false
URxvt*font:			xft:Inconsolata:normal:size=24
URxvt*letterSpace:	-1

! X colors.
! Generated by 'wal'
*foreground:        #e0cef3
*background:        #040516
*.foreground:       #e0cef3
*.background:       #040516
emacs*foreground:   #e0cef3
emacs*background:   #040516
URxvt*foreground:   #e0cef3
XTerm*foreground:   #e0cef3
UXTerm*foreground:  #e0cef3
URxvt*background:   [85]#040516
XTerm*background:   #040516
UXTerm*background:  #040516
URxvt*cursorColor:  #e0cef3
XTerm*cursorColor:  #e0cef3
UXTerm*cursorColor: #e0cef3
URxvt*borderColor:  [85]#040516

! Colors 0-15.
*.color0: #040516
*color0:  #040516
