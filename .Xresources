! Black color that will not be affected by bold highlighting.
*.color66: #040516
*color66:  #040516

! Xclock colors.
XClock*foreground: #e0cef3
XClock*background: #040516
XClock*majorColor:  rgba:e0/ce/f3/ff
XClock*minorColor:  rgba:e0/ce/f3/ff
XClock*hourColor:   rgba:e0/ce/f3/ff
XClock*minuteColor: rgba:e0/ce/f3/ff
XClock*secondColor: rgba:e0/ce/f3/ff

! Set depth to make transparency work.
URxvt*depth: 32
