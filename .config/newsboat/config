unbind-key p

bind-key G end
bind-key J next-feed articlelist
bind-key K prev-feed articlelist
bind-key N prev-unread
bind-key U show-urls
bind-key ^B bookmark
bind-key a toggle-article-read
bind-key d pagedown
bind-key g home
bind-key h quit
bind-key j down
bind-key j next articlelist
bind-key k prev articlelist
bind-key k up
bind-key l open
bind-key n next-unread
bind-key u pageup
bind-key x delete-article

browser "${BROWSER}"

cache-file "~/.cache/newsboat/cache.db"

cleanup-on-quit yes

color article default default
color title color12 color0
color info color12 color0
color listfocus color12 default standout
color listfocus_unread color12 default bold standout
color listnormal color10 default
color listnormal_unread cyan default

highlight feedlist "===.*===" default default dim

macro , open-in-browser
macro h set browser "linkhandler %u"; open-in-browser; set browser "${BROWSER}"
# (c)opy URL
macro c set browser "printf %s %u | xclip -selection clipboard"; open-in-browser; set browser "${BROWSER}"

player "mpv"

reload-threads 4

save-path "~/books/newsboat/"

text-width 72

proxy 127.0.0.1:9050
proxy-type socks5h
use-proxy yes
download-timeout 60
