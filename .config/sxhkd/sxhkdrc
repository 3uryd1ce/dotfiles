super + Return
	st

super + shift + f
	firefox -P default-default

super + shift + w
	tor-browser

super + d
	${HOME}/bin/dmen

super + u
	${HOME}/bin/dmenuunicode

super + shift + p
	keepassxc

super + n
	pkill -USR1 xidle

Print
	${HOME}/bin/maimpick

super + greater
	mpc next

super + less
	mpc prev

shift + super + t
	mpc toggle

super + plus
	mpc seek 0

F1
	sndioctl output.mute=! && pkill -USR1 i3status

F2
	sndioctl output.level=-0.05 && pkill -USR1 i3status

F3
	sndioctl output.level=+0.05 && pkill -USR1 i3status

F5
	xbacklight -dec 10

F6
	xbacklight -inc 10
